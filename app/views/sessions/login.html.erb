<form action="sessions/check_user" method="post">
			<legend>Login</legend>
			<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
			<p>
				<label>Email:</label>
				<input type="text" name="email">
			</p>
			<p>
				<label>Password:</label>
				<input type="password" name="password">
			</p>
			<p>
				<button>Login</button>
			</p>
			<a href="/online_landing/register">Click here to register?</a>
	</form>